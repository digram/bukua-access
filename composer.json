{
    "name": "digram/bukua-access",
    "type": "library",
    "description": "Access Bukua Edtech API services for your Laravel application",
    "keywords": [
        "bukua",
        "edtech",
        "kenya",
        "laravel",
        "apis"
    ],
    "homepage": "https://www.bukuaplatform.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Bukua Platform",
            "homepage": "https://www.bukuaplatform.com",
            "email": "dennismayeku@gmail.com"
        }
    ],
    "support": {
        "docs": "https://github.com/digram/bukua-access"
    },
    "require": {
        "php": "^5.6||^7.0||^8.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "BukuaAccess\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BukuaAccess\\Providers\\BukuaAccessServiceProvider",
                "BukuaAccess\\Providers\\BukuaAccessEventServiceProvider"
            ]
        }
    }
}